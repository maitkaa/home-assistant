generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

enum MeasurePoint {
  BEDROOM
  LIVING_ROOM
  OUTSIDE
}

model Mesurements {
  id            String       @id @default(cuid())
  measurePoints MeasurePoint
  value         Float
  createdAt     DateTime     @default(now())
  updatedAt     DateTime     @updatedAt
}
